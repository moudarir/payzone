{
  "name": "moudarir/payzone",
  "description": "PHP library for integration with the Payzone payment gateway in a fast and simple way.",
  "keywords": [
    "php",
    "Payzone",
    "SDK",
    "library"
  ],
  "type": "library",
  "license": "MIT",
  "config": {
    "platform":{
      "php": "7.2"
    },
    "bin-dir": "vendor/bin"
  },
  "authors": [
    {
      "name": "Abdessamad MOUDARIR",
      "email": "amoudarir@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^6.4",
    "nesbot/carbon": "^2.27",
    "tightenco/collect": "^6.6",
    "ext-mcrypt": "*"
  },
  "autoload": {
    "psr-4": {
      "Moudarir\\Payzone\\": "src/"
    }
  },
  "require-dev": {
    "symfony/var-dumper": "^4.4",
    "filp/whoops": "^2.5"
  }
}
